{
  "name": "@tencent/honeycli",
  "version": "1.3.1",
  "description": "a frontend cli toolkit",
  "main": "cli.js",
  "bin": {
    "honey": "cli.js"
  },
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tb-fed/tb-cli.git"
  },
  "author": "nicolasxiao",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/tb-fed/tb-cli/issues"
  },
  "homepage": "https://github.com/tb-fed/tb-cli#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "commitizen": "^4.2.2",
    "cross-spawn": "^7.0.3",
    "inquirer": "^7.3.3",
    "is-core-module": "^2.2.0",
    "standard-version": "^9.0.0",
    "superagent": "^6.1.0",
    "superagent-proxy": "^2.0.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^26.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false,
      "maxHeaderWidth": 100,
      "maxLineWidth": 100,
      "defaultType": "",
      "defaultScope": "",
      "defaultSubject": "",
      "defaultBody": "",
      "defaultIssues": "",
      "types": {
        "feat": {
          "description": "A new feature",
          "title": "Features"
        },
        "fix": {
          "description": "A new fix",
          "title": "Fix"
        }
      }
    }
  }
}
